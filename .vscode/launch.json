{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Serverless Offline",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless.js",
      "args": ["offline", "start"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
